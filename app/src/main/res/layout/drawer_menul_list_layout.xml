                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp">

                <TextView
                    android:id="@+id/text_vouchers"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toEndOf="@+id/imageView_voucher"
                    android:layout_toRightOf="@+id/imageView_voucher"
                    android:fontFamily="@font/gillsansbold"
                    android:gravity="center_vertical"
                    android:paddingLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingRight="5dp"
                    android:paddingBottom="15dp"
                    android:text="Vouchers"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/_12ssp" />

                <ImageView
                    android:id="@+id/imageView_voucher"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="0dp"
                    android:layout_marginLeft="0dp"
                    android:src="@drawable/vouchers_white" />

                <TextView
                    android:id="@+id/txtVoucherCount"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/text_vouchers"
                    android:layout_toRightOf="@+id/text_vouchers"
                    android:background="@drawable/drawer_badge_shape"
                    android:gravity="center"
                    android:text="1"
                    android:layout_marginLeft="5dp"
                    android:textColor="@color/colorWhite"
                    android:textSize="11sp"
                    android:textStyle="bold"
                    android:visibility="gone" />

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/layoutPromotions"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/black"
                android:layout_marginLeft="20dp">

                <TextView
                    android:id="@+id/text_promotions"
