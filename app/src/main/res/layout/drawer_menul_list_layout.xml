<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="240dp"
    android:layout_height="match_parent"
    android:clickable="true"
    android:paddingLeft="5dp"
    android:background="@color/black"
    android:paddingTop="10dp">


    <ScrollView
        android:scrollbars="none"
        android:id="@+id/navigationmenuScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="20dp"
            android:orientation="vertical"
            android:visibility="visible">

            <RelativeLayout
                android:id="@+id/layoutHome"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp">

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toEndOf="@+id/imageView_ic_home"
                    android:layout_toRightOf="@+id/imageView_ic_home"
                    android:gravity="center_vertical"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:paddingTop="15dp"
                    android:fontFamily="@font/gillsansbold"
                    android:text="Home"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/_12ssp" />

                <ImageView
                    android:id="@+id/imageView_ic_home"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/home_white" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layoutAccount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toEndOf="@+id/imageView_account"
                    android:layout_toRightOf="@+id/imageView_account"
                    android:fontFamily="@font/gillsansbold"
                    android:gravity="center_vertical"
                    android:paddingLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingRight="5dp"
                    android:paddingBottom="15dp"
                    android:text="My Account"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/_12ssp" />

                <ImageView
                    android:id="@+id/imageView_account"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/myaccount_white" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layoutViewOrder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp">

                <TextView
                    android:id="@+id/text_order"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toEndOf="@+id/imageView_vieworder"
                    android:layout_toRightOf="@+id/imageView_vieworder"
                    android:fontFamily="@font/gillsansbold"
                    android:gravity="center_vertical"
                    android:paddingLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingRight="5dp"
                    android:paddingBottom="15dp"
                    android:text="View Orders"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/_12ssp" />

                <ImageView
                    android:id="@+id/imageView_vieworder"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/view_orders_white" />

                <TextView
                    android:id="@+id/txtOrderCount"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/text_order"
                    android:layout_toRightOf="@+id/text_order"
                    android:background="@drawable/drawer_badge_shape"
                    android:gravity="center"
                    android:text="2"
                    android:layout_marginLeft="5dp"
                    android:textColor="@color/colorWhite"
                    android:textSize="11sp"
                    android:visibility="gone"
                    android:textStyle="bold" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layoutRewards"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp">

                <TextView
                    android:id="@+id/text_rewards"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toEndOf="@+id/imageView_rewards"
                    android:layout_toRightOf="@+id/imageView_rewards"
                    android:gravity="center_vertical"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:paddingTop="15dp"
                    android:text="Rewards"
                    android:fontFamily="@font/gillsansbold"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/_12ssp" />

                <ImageView
                    android:id="@+id/imageView_rewards"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/rewards_white"/>

                <TextView
                    android:id="@+id/count_rewards"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/text_rewards"
                    android:layout_toRightOf="@+id/text_rewards"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:textSize="11sp" />

            </RelativeLayout>

            <RelativeLayout
