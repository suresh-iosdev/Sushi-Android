                    android:textSize="15sp" />

                <ImageView
                    android:id="@+id/imageView_whatshappening"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="0dp"
                    android:layout_marginLeft="0dp"
                    android:src="@drawable/asset42" />

                <TextView
                    android:id="@+id/txtWhatHappeningCount"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/text_whatshappening"
                    android:layout_toRightOf="@+id/text_whatshappening"
                    android:background="@drawable/drawer_badge_shape"
                    android:gravity="center"
                    android:text="1"
                    android:layout_marginLeft="5dp"
                    android:textColor="@color/colorWhite"
                    android:textSize="11sp"
                    android:textStyle="bold"
                    android:visibility="gone" />

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/happingLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:visibility="gone">

                <TextView
                    android:fontFamily="@font/gillsans"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toEndOf="@+id/imageView_whathappening"
                    android:layout_toRightOf="@+id/imageView_whathappening"
                    android:gravity="center_vertical"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:paddingTop="15dp"
                    android:text="What's Happening"
                    android:textColor="#000000"
                    android:textSize="15sp" />

                <ImageView
                    android:id="@+id/imageView_whathappening"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layoutNotification"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp">

                <TextView
                    android:id="@+id/text_notification"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toEndOf="@+id/imageView_notification"
                    android:layout_toRightOf="@+id/imageView_notification"
                    android:gravity="center_vertical"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:paddingTop="15dp"
                    android:text="Notifications"
                    android:textColor="@color/colorWhite"
                    android:fontFamily="@font/gillsansbold"
                    android:textSize="@dimen/_12ssp" />

                <ImageView
                    android:id="@+id/imageView_notification"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/notification_white" />

                <TextView
                    android:id="@+id/txtNotificationCount"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/text_notification"
                    android:layout_toRightOf="@+id/text_notification"
                    android:background="@drawable/drawer_badge_shape"
                    android:gravity="center"
                    android:text="3"
                    android:visibility="gone"
                    android:layout_marginLeft="5dp"
                    android:textColor="@color/colorWhite"
                    android:textSize="11sp"
                    android:textStyle="bold" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layoutAbout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toEndOf="@+id/imageView_about"
                    android:layout_toRightOf="@+id/imageView_about"
                    android:gravity="center_vertical"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:paddingTop="15dp"
                    android:text="About"
                    android:textColor="@color/colorWhite"
                    android:fontFamily="@font/gillsansbold"
                    android:textSize="@dimen/_12ssp" />

                <ImageView
                    android:id="@+id/imageView_about"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:tint="@color/colorWhite"
                    android:src="@drawable/asset43" />

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="@dimen/_3sdp"
                    android:paddingRight="10dp"
                    android:rotation="180"
                    android:src="@drawable/next"
                    android:visibility="gone" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_terms"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toEndOf="@+id/imageView_terms"
                    android:layout_toRightOf="@+id/imageView_terms"
                    android:gravity="center_vertical"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:paddingTop="15dp"
                    android:text="Terms &amp; Conditions"
                    android:textColor="@color/colorWhite"
                    android:fontFamily="@font/gillsansbold"
                    android:textSize="@dimen/_12ssp" />

                <ImageView
                    android:id="@+id/imageView_terms"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:tint="@color/colorWhite"
                    android:src="@drawable/terms_icon" />



            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_refund"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toEndOf="@+id/imageView_refund"
                    android:layout_toRightOf="@+id/imageView_refund"
