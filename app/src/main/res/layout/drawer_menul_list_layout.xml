            <RelativeLayout
                android:id="@+id/loginLogoutLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_alignParentBottom="true">

                <LinearLayout
                    android:id="@+id/layoutSignIn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone"
                    android:weightSum="2">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp">

                        <TextView
                            android:id="@+id/login"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dp"
                            android:layout_toEndOf="@+id/login_image_logo"
                            android:layout_toRightOf="@+id/login_image_logo"
                            android:gravity="center_vertical|center"
                            android:paddingLeft="5dp"
                            android:paddingRight="5dp"
                            android:text="Login"
                            android:fontFamily="@font/gillsansbold"
                            android:textColor="@color/colorBlack"
                            android:textSize="15sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/login"
                            android:layout_below="@+id/login"
                            android:layout_centerVertical="true"
                            android:layout_marginTop="4dp"
                            android:gravity="center_vertical|center"
                            android:maxLines="2"
                            android:paddingLeft="@dimen/_3sdp"
                            android:paddingRight="@dimen/_30sdp"
                            android:textSize="@dimen/_12ssp"
                            android:textColor="@color/colorWhite"
                            android:fontFamily="@font/gillsansbold"
                            android:id="@+id/loggedinemail"
                            android:verticalScrollbarPosition="right"
                            android:visibility="visible" />


                            <ImageView
                                android:id="@+id/login_image_logo"
                                android:layout_width="18dp"
                                android:layout_height="18dp"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_centerVertical="true"
                                android:adjustViewBounds="true"
                                android:src="@drawable/logout_unselect" />

                    </RelativeLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layoutSignOut"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_below="@id/layoutSignIn"
                    android:layout_marginBottom="@dimen/_10sdp"
                    android:visibility="visible"
                    android:weightSum="2">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                       >

                        <TextView
                            android:id="@+id/txtLogout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="5dp"
                            android:layout_toEndOf="@+id/logout_image_logo"
                            android:layout_toRightOf="@+id/logout_image_logo"
                            android:fontFamily="@font/gillsansbold"
                            android:gravity="center_vertical|left"
                            android:paddingLeft="5dp"
                            android:paddingTop="15dp"
                            android:paddingRight="5dp"
                            android:paddingBottom="15dp"
                            android:text="Logout"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_12ssp" />

                        <TextView
                            android:id="@+id/txtSignedUsername"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/txtLogout"
                            android:layout_below="@+id/txtLogout"
                            android:layout_marginTop="@dimen/_1sdp"
                            android:fontFamily="@font/gillsansmedium"
                            android:textColor="#797979"
